{
  "$schema": "../../../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "YASCAR",
  "version": "0.1.0",
  "identifier": "com.yascar.app",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "YASCAR",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false,
        "visible": true
      }
    ],
    "trayIcon": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": false,
      "id": "main-tray"
    },
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "endpoints": [
        "https://github.com/KeeganFargher/YASCAR/releases/latest/download/latest.json"
      ]
    }
  }
}
