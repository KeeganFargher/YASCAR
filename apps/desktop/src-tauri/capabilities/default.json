{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "store:default",
    "autostart:default",
    "notification:default",
    "updater:default",
    "dialog:default",
    "process:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://shift.gearboxsoftware.com/**" },
        { "url": "https://shift.keeganfargher.co.za/**" },
        { "url": "https://github.com/KeeganFargher/YASCAR/releases/**" }
      ]
    }
  ]
}
